{
  "name": "mfm",
  "version": "3.3.0",
  "description": "File management application. You can bulk rename or deleting your files in at the same time, with certain condition. Available for windows, linux, and macos",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "start": "npm run electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon/icon.png --output=build --flatten"
  },
  "author": {
    "name": "Dhany Nurdiansyah",
    "email": "dhanyn.dhan@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhanyn10/multiple-file-manager.git"
  },
  "bugs": {
    "url": "https://github.com/dhanyn10/multiple-file-manager/issues"
  },
  "homepage": "https://github.com/dhanyn10/multiple-file-manager#readme",
  "main": "background.js",
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "chokidar": "^3.5.1",
    "trash": "^7.1.1",
    "vue": "^2.6.13"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "babel-eslint": "^10.1.0",
    "electron": "^12.0.7",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.30.0",
    "eslint-plugin-vue": "^7.10.0",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-template-compiler": "^2.6.13"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
