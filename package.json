{
  "name": "multiple-file-manager",
  "version": "2.0.0",
  "description": "windows application made with electron to modify all of your file in a specified folder",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . multiple-file-manager --overwrite --asar=true --platform=win32 --arch=ia32 --icon=multiple-file-manager.ico  --version-string.CompanyName=DhanyN --version-string.ProductName=\"Multiple File Manager\"",
    "package-linux": "electron-packager . multiple-file-manager --overwrite --asar=true --platform=linux --arch=x64 --icon=multiple-file-manager.ico  --version-string.CompanyName=DhanyN --version-string.ProductName=\"Multiple File Manager\"",
    "package-mac": "electron-packager . multiple-file-manager --overwrite --asar=true --platform=darwin --arch=x64 --icon=multiple-file-manager.ico  --version-string.CompanyName=DhanyN --version-string.ProductName=\"Multiple File Manager\"",
    "dist": "electron-builder --publish=never"
  },
  "build": {
    "productName": "Multiple File Manager"
  },
  "keywords": [
    "file",
    "file management",
    "editor"
  ],
  "author": "Dhany Nurdiansyah",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.6.3",
    "electron": "2.0.17",
    "electron-builder": "20.38.5",
    "electron-packager": "13.1.1"
  },
  "dependencies": {
    "bootbox": "^4.4.0",
    "bootsalert": "^0.1.0",
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhanyn10/multiple-file-manager.git"
  },
  "bugs": {
    "url": "https://github.com/dhanyn10/multiple-file-manager/issues"
  },
  "homepage": "https://github.com/dhanyn10/multiple-file-manager#readme"
}
