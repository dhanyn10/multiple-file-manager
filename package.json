{
  "name": "multiple-file-manager",
  "version": "2.0.0",
  "description": "windows application made with electron to modify all of your file in a specified folder",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . multiple-file-manager --overwrite --asar=true --platform=win32 --arch=ia32 --icon=multiple-file-manager.ico  --version-string.CompanyName=DhanyN --version-string.FileDescription=DhanyN --version-string.ProductName=\"Multiple File Manager\"",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "Multiple File Manager",
    "icon": "multiple-file-manager.ico"
  },
  "keywords": [
    "file",
    "file management",
    "editor"
  ],
  "author": "Dhany Nurdiansyah",
  "license": "MIT",
  "devDependencies": {
    "electron": "2.0.12",
    "electron-builder": "20.28.3"
  },
  "dependencies": {
    "bootbox": "^4.4.0",
    "bootsalert": "^0.1.0",
    "bootstrap": "^3.3.7",
    "jquery": "^3.3.1"
  }
}