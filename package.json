{
  "name": "multiple-file-manager",
  "version": "2.0.2",
  "description": "application made with electron to modify all of your file in a specified folder",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test-dist": "electron-builder --publish=never"
  },
  "build": {
    "productName": "Multiple File Manager"
  },
  "keywords": [
    "file",
    "file management",
    "editor"
  ],
  "author": "Dhany Nurdiansyah",
  "license": "MIT",
  "devDependencies": {
    "electron": "7.2.4",
    "electron-builder": "22.3.2"
  },
  "dependencies": {
    "bootsalert": "^0.1.0",
    "bootstrap": "^4.2.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.14.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhanyn10/multiple-file-manager.git"
  },
  "bugs": {
    "url": "https://github.com/dhanyn10/multiple-file-manager/issues"
  },
  "homepage": "https://github.com/dhanyn10/multiple-file-manager#readme"
}
